//Vectors of Vectors

#include<iostream>
#include<vector>
using namespace std;

int main(){
	vector<vector<int>> arr ={
		{1,2,3},
		{5,6,7,8},
		{2,4},
		{3,6,7}};

		arr[0][0] +=10; //can update;

		for(int i=0; i<arr.size(); i++)
		{
			for(int number : arr[i])
			{
				cout<<number<<",";
			}
			cout<<endl;

		}

		return 0;
}
